{
    "@context": {
        "@base": "http://example.org/TBD#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "ot": "http://purl.org/opentree/nexson#",
        "cdao": "http://purl.obolibrary.org/obo/CDAO_",
        "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
        "tbd": "http://example.org/TBD#",
        
        "curator": {
            "@id": "ot:curatorName",
            "@type": "xsd:string"
        },
        "comments": {
            "@id": "ot:comment",
            "@type": "xsd:string"
        },

        "citation": "ot:studyPublicationReference",
        "url": {
            "@id": "ot:studyPublication",
            "@type": "@id"
        },
        "year": {
            "@id": "ot:studyYear",
            "@type": "xsd:integer"
        },

        "phylogenies": {
            "@id": "tbd:phylogenies",
            "@container": "@list"
        },

        "otus": {
            "@id": "tbd:otus",
            "@container": "@set"
        },

        "phylorefs": {
            "@id": "tbd:phylorefs",
            "@container": "@set"
        },

        "newick": {
            "@id": "tbd:asNewickString",
            "@type": "xsd:string"
        },

        "cladeDefinition": {
            "@id": "tbd:cladeDefinition",
            "@type": "xsd:string"
        },

        "internalSpecifiers": {
            "@id": "tbd:internalSpecifiers",
            "@container": "@set"
        },

        "externalSpecifiers": {
            "@id": "tbd:externalSpecifiers",
            "@container": "@set"
        },

        "label": "rdfs:label",
        "binomialName": "tbd:binomialName",
        "NCBITaxonId": "tbd:NCBITaxonId",
        "uBioNameBankId": "tbd:uBioNameBankId",
        "GenBankAccession": "tbd:GenBankAccession",
        "voucheredSpecimen": "tbd:voucheredSpecimen",
        "collectionDetails": "tbd:collectionDetails",

        "originalFile": "tbd:originalFile"
    }
}
